#import <Foundation/Foundation.h>
#import <CoreData/CoreData.h>


@interface Notes : NSManagedObject

@property (nonatomic, retain) NSString * noteContent;
@property (nonatomic, retain) NSDate * noteCreationDate;
@property (nonatomic, retain) NSString * noteID;
@property (nonatomic, retain) NSDate * noteModifiedDate;
@property (nonatomic, retain) NSString * noteModule;
@property (nonatomic, retain) NSString * noteRelatedToID;
@property (nonatomic, retain) NSString * noteTitle;
@property (nonatomic, retain) NSNumber * notesCRMSyncStatus;
@property (nonatomic, retain) NSString * noteCreatorName;
@property (nonatomic, retain) NSString * noteModifierName;
@property (nonatomic, retain) NSNumber * isVoiceNotes;

@end
